{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Security-Policy", "value": "frame-ancestors https://web.telegram.org https://*.telegram.org https://*.t.me https://t.me" },
        { "key": "Referrer-Policy", "value": "no-referrer-when-downgrade" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" }
      ]
    }
  ]
}
